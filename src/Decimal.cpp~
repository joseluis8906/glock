#include "Integer.h"

Glock::Decimal::Decimal () { }

Glock::Decimal::Decimal (int target) : value{(signed long double)target} { }

Glock::Decimal::Decimal (long int target) : value{(signed long double)target} { }

Glock::Decimal::Decimal (long long int target) : value{(signed long double)target} { }

Glock::Decimal::Decimal (float target) : value{(signed long double)target} { }

Glock::Decimal::Decimal (double target) : value{(signed long double)target} { }

Glock::String Glock::Integer::ToString () const {
    return std::to_string (value);
}

